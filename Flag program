#include <stdio.h>

int main()

{

    int a[10];

    int i,j,flag=0;

    printf("enter numbers");

    for(i=0;i<5;i++)

    {

        scanf("%d",&a[i]);

    }

    for(i=4;i>=0;i--)

    {

        flag=0;

        for(j=2;j<a[i];j++)

        {

            if(a[i]%j==0)

            {

                flag=1;

                break;

            }

        }

        if(flag==0)

        

        printf("%d\t",a[i]);

    }

    return 0;

}

